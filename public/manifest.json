{
  "name": "tabtab",
  "description": "A simple Chrome extension to manage and save tabs. Workona like tab manager.",
  "manifest_version": 3,
  "version": "0.0.1",
  "action": {},
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["cookies", "tabs", "storage"],
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  }
}
